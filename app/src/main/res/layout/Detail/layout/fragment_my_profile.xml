<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:float="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UICore.Detail.MyProfileFragment">



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/padding1">


            <com.wrapp.floatlabelededittext.FloatLabeledEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                float:fletPadding="5dp">

                <EditText
                    android:id="@+id/edt_first_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="First Name"
                    android:background="@drawable/field_back"
                    android:textColor="#000000"
                    android:singleLine="true"
                    android:enabled="false"
                    android:textCursorDrawable="@drawable/cursor_colour"
                    android:textSize="@dimen/textsize2"
                    android:padding="@dimen/padding1"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"/>

            </com.wrapp.floatlabelededittext.FloatLabeledEditText>

            <!--android:text="pprasoon"-->

            <com.wrapp.floatlabelededittext.FloatLabeledEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                float:fletPadding="5dp">

                <EditText
                    android:id="@+id/edt_last_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Last Name"
                    android:background="@drawable/field_back"
                    android:textColor="#000000"
                    android:singleLine="true"
                    android:enabled="false"
                    android:textCursorDrawable="@drawable/cursor_colour"
                    android:textSize="@dimen/textsize2"
                    android:padding="@dimen/padding1"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp" />

            </com.wrapp.floatlabelededittext.FloatLabeledEditText>

            <com.wrapp.floatlabelededittext.FloatLabeledEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                float:fletPadding="5dp">


                <EditText
                    android:id="@+id/edt_mobile_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Mobile Number"
                    android:background="@drawable/field_back"
                    android:textColor="#000000"
                    android:singleLine="true"
                    android:inputType="none"
                    android:enabled="false"
                    android:textCursorDrawable="@drawable/cursor_colour"
                    android:textSize="@dimen/textsize2"
                    android:padding="@dimen/padding1"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp" />

            </com.wrapp.floatlabelededittext.FloatLabeledEditText>

            <com.wrapp.floatlabelededittext.FloatLabeledEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                float:fletPadding="5dp">

                <EditText
                    android:id="@+id/edt_email_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Email"
                    android:background="@drawable/field_back"
                    android:textColor="#000000"
                    android:singleLine="true"
                    android:inputType="none"
                    android:enabled="false"
                    android:textCursorDrawable="@drawable/cursor_colour"
                    android:textSize="@dimen/textsize2"
                    android:padding="@dimen/padding1"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp" />

            </com.wrapp.floatlabelededittext.FloatLabeledEditText>

            <!--android:text="1234"-->

